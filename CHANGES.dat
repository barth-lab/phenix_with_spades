echo "Copying files into phenix needed for rosetta"
cp ${input}/refine.py ${input}/utils.py ${input}/xray_target.py ${phenix_loc}/phenix/install_dir/phenix-1.19.2-4158/modules/phenix/phenix/rosetta/

